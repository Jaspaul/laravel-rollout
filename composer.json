{
    "name": "jaspaul/laravel-rollout",
    "description": "A package to integrate rollout into your Laravel project.",
    "keywords": ["laravel", "rollout"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "illuminate/cache": "^7|^8|^9",
        "illuminate/config": "^7|^8|^9",
        "illuminate/console": "^7|^8|^9",
        "illuminate/database": "^7|^8|^9",
        "illuminate/support": "^7|^8|^9",
        "opensoft/rollout": "2.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1",
        "illuminate/container": "^7|^8|^9",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Jaspaul\\LaravelRollout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaspaul\\LaravelRollout\\ServiceProvider"
            ]
        }
    }
}
