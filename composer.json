{
    "name": "jaspaul/laravel-rollout",
    "description": "A package to integrate rollout into your Laravel project.",
    "keywords": ["laravel", "rollout"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/cache": "^5.4",
        "illuminate/config": "^5.4",
        "illuminate/console": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4",
        "opensoft/rollout": "^2.2"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4",
        "friendsofphp/php-cs-fixer": "^2.1",
        "illuminate/container": "^5.4",
        "mockery/mockery": "^0.9.9",
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Jaspaul\\LaravelRollout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaspaul\\LaravelRollout\\ServiceProvider"
            ]
        }
    }
}
